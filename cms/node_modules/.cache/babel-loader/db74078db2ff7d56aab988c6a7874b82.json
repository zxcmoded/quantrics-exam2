{"ast":null,"code":"import assetApi from '../../api/assetApi';\nimport loading from 'vue-loading-overlay';\nexport default {\n  props: {\n    asset: {}\n  },\n  components: {\n    loading\n  },\n  name: 'edit-asset-dialog',\n  watch: {\n    asset(value) {\n      this.form.Id = value.id;\n      this.form.Name = value.name;\n      this.form.Price = value.price;\n      this.form.ValidFrom = value.validFrom ? new Date(value.validFrom).toISOString().split('T')[0] : null;\n      this.form.ValidTo = value.validTo ? new Date(value.validTo).toISOString().split('T')[0] : null;\n    }\n\n  },\n\n  data() {\n    return {\n      form: {\n        Id: null,\n        Name: '',\n        Price: 0,\n        ValidFrom: null,\n        ValidTo: null\n      },\n      isLoading: false\n    };\n  },\n\n  methods: {\n    async handleOk(bvModalEvent) {\n      this.isLoading = true;\n\n      try {\n        bvModalEvent.preventDefault();\n        const {\n          data\n        } = await assetApi.updateAsset(this.form);\n\n        if (data.success) {\n          this.$toastr.success(data.message, 'Information');\n          this.$emit('loadList', data);\n          this.$bvModal.hide('modal-edit-asset');\n        } else {\n          this.$toastr.warning(data.message, 'Warning');\n        }\n      } catch (error) {\n        this.$toastr.error('Error updating asset', 'Error');\n      }\n\n      this.isLoading = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmDA;AACA;AAEA;EACAA;IACAC;EADA,CADA;EAIAC;IACAC;EADA,CAJA;EAOAC,yBAPA;EAQAC;IACAJ;MACA;MACA;MACA;MACA;MACA;IACA;;EAPA,CARA;;EAiBAK;IACA;MACAC;QACAC,QADA;QAEAC,QAFA;QAGAC,QAHA;QAIAC,eAJA;QAKAC;MALA,CADA;MAQAC;IARA;EAUA,CA5BA;;EA6BAC;IACA;MACA;;MACA;QACAC;QAEA;UAAAT;QAAA;;QACA;UACA;UACA;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CAXA,CAWA;QACA;MACA;;MACA;IACA;;EAlBA;AA7BA","names":["props","asset","components","loading","name","watch","data","form","Id","Name","Price","ValidFrom","ValidTo","isLoading","methods","bvModalEvent"],"sourceRoot":"sr